// 6

int main()
{
    int n,k=0;
    printf("n=");
    scanf("%d",&n);
    while(n)
    {
        if(n&1)
            k++;
        n=n>>1;
    }
    if(k==1)
        printf("n este o putere a lui 2");
    else
        printf("n nu este o putere a lui 2");
    return 0;
}
